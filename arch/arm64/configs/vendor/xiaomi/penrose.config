# Device Specific Board Configuration
CONFIG_BOARD_PENROSE=y

# Touchscreen
CONFIG_TOUCHSCREEN_FOCALTECH=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_TEE=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Enable MI US Proximity sensor
CONFIG_US_PROXIMITY=y

# Enable Thermal Dimming feature
CONFIG_THERMAL_DIMMING=y

# Power supply drivers
# CONFIG_QPNP_SMB5 is not set
# CONFIG_QPNP_QNOVO5 is not set
# CONFIG_QPNP_FG_GEN4 is not set
CONFIG_ARCH_CAS_QCOM=y
CONFIG_SMB1398_CHARGER=y
CONFIG_QPNP_SMB5_CAS=y
CONFIG_CHARGER_BQ25790=y
CONFIG_FUEL_GAUGE_BQ28Z610=y
CONFIG_WATCHDOG_MAX28200=y
CONFIG_CP_MAX77932=y

# Disable FSA4480 switch
# CONFIG_QCOM_FSA4480_I2C is not set

